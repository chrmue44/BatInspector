<Window x:Class="BatInspector.Forms.frmFindBat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatInspector.Controls"
        xmlns:res="clr-namespace:BatInspector.Properties"
        mc:Ignorable="d"
        Title="{x:Static res:MyResources.FindBatNarrowDown}" Height="414.912" Width="617.309">
    <StackPanel>
    <StackPanel Orientation="Horizontal" Margin="5,10,0,0">
      <local:ctlDataItem x:Name="_ctlCharFreq" ></local:ctlDataItem>
      <CheckBox Name="_cbCharFreq" Content="enable" Margin="10,5,0,0"></CheckBox>
    </StackPanel>
    <StackPanel Orientation="Horizontal" Margin="5,5,0,0">
      <local:ctlDataItem x:Name="_ctlDuration" ></local:ctlDataItem>
      <CheckBox Name="_cbDuration" Content="enable" Margin="10,5,0,0"></CheckBox>
    </StackPanel>
    <StackPanel Orientation="Horizontal" Margin="5,5,0,0">
      <local:ctlDataItem x:Name="_ctlCallDist" ></local:ctlDataItem>
      <CheckBox Name="_cbCallDist" Content="enable" Margin="10,5,0,0"></CheckBox>
    </StackPanel>
    <Label Content="{x:Static res:MyResources.FindBatPossibleSpecies}" Margin="5,10"></Label>
    <DataGrid Name="_dgSpecies" Height="269"></DataGrid>
  </StackPanel>
</Window>
