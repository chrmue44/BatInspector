<Window x:Class="BatInspector.Forms.frmModelParams"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:res="clr-namespace:BatInspector.Properties"
        mc:Ignorable="d"
        Title="{x:Static res:MyResources.frmModParsTitle}" Height="450" Width="800">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <ScrollViewer Grid.Row="0">
        <StackPanel Name="_sp"/>
        </ScrollViewer>
        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button x:Name="_btnCancel" Content="{x:Static res:MyResources.BtnCancel}" Width="150" Margin="0,5,10,0" Click="_btnCancel_Click" />
            <Button x:Name="_btnOK" Content="{x:Static res:MyResources.BtnOK}" Width="150" Margin="0,5,30,0" Click="_btnOK_Click" />

        </StackPanel>
    </Grid>
</Window>
