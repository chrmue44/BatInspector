<Window x:Class="BatInspector.Forms.frmScriptParams"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatInspector.Forms"
        xmlns:res="clr-namespace:BatInspector.Properties"
        mc:Ignorable="d"
        Title="Skript Parameter" Height="250" Width="600">
        <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30" />
        </Grid.RowDefinitions>
        <StackPanel Name="_sp" Grid.Row="0" Orientation="Vertical" Margin="0,0,0,20" />
        <StackPanel Grid.Row="1" Orientation="Horizontal"  HorizontalAlignment="Right" Margin="10,0,0,0" >
            <Button Name="_btnCancel" Width="100" Content="{x:Static res:MyResources.BtnCancel}" Margin="10,0" Click="_btnCancel_Click" />
            <Button Name="_btnOk" Width="100" Content="Start" Click="_btnOk_Click"/>
        </StackPanel>
    </Grid>
</Window>
