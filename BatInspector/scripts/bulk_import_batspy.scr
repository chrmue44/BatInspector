SET srcDir =(PAR1)
SET dstDir =(PAR2)
SET maxFiles 1000
SET maxFileLen 500

SET dirCount =(countDirs(srcDir,"*") - 1)
LOG =("found " + (dirCount + 1) + " projects")

FOR i 0 dirCount
  SET prjDir =(getDir(i))
  SET dstPrj =(dstDir + "/" + getDir(i, 1))
  LOG =("importing " + prjDir + " ...")
  =(importPrj(prjDir, dstDir, maxFiles, maxFileLen))
  =(openPrj(dstPrj))
  =(inspectPrj())
END
LOG =("bulk import completed")