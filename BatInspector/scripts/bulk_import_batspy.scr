SET srcDir =(PAR1)
SET dstDir =(PAR2)
SET location =(PAR3)
SET creator =(PAR4)
SET notes =(PAR5)
SET maxFiles 50000
SET maxFileLen 5

SET dirCount =(countDirs(srcDir,"*"))
LOG =("found " + (dirCount + 1) + " sub directories")

FOR i 0 dirCount
  SET prjDir =(getDir(i))
  SET dstPrj =(dstDir + "/" + getDir(i, 1))
  LOG =("importing " + prjDir + " ...")
  =(importPrj(prjDir, dstDir, maxFiles, maxFileLen, location, creator, notes))
END
LOG =("bulk import completed")