#replace all calls marked with 'todo' with a new value
SET NrFiles =(getPrjFileCount() - 1)
SET Count 0
SET newSpecies ?
FOR FileNr 0 NrFiles
  SET NrCalls =(getCallCount(FileNr) - 1)
  FOR i 0 NrCalls
	  SET speciesMan =(getCallInfo(FileNr, i, "SPEC_MAN"))
	  IF =(speciesMan == "TODO")
        =(setCallInfo(FileNr, i, "SPEC_MAN", newSpecies))
        SET Count =(Count + 1)
      END
	END
  END
END
LOG =("replaced " + Count + " todos") 
