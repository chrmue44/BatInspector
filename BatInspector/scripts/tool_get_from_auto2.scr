# Tool: set all species man to Species2
SET FileNr =(getFileIndex(VAR_FILE_NAME))
SET NrOfCalls =(getCallCount(FileNr) - 1)

FOR i 0 NrOfCalls
  SET specAuto =(getCallInfo(FileNr, i, "SPEC_AUTO2"))
  LOG =("specAuto:" + specAuto)
  SET specMan =(getCallInfo(FileNr, i, "SPEC_MAN"))
  IF =(specMan == "TODO")
    IF =(indexOf(specAuto, "??") >= 0)
      =(setCallInfo(FileNr, i, "SPEC_MAN", "?"))
	ELSE
      =(setCallInfo(FileNr, i, "SPEC_MAN", specAuto))
    END
  END	
END
